		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Bottler Gross Profit / Standard Physical Case - Plan' = divide([Bottler Gross Profit],[Standard Cases - Plan])
		formatString: #,0.00
		lineageTag: f68da0d8-d832-46e8-b04b-da38a66bd679

	measure 'Bottler Revenue' = SUM('Customer Invoice Measures'[bottler_dead_net_net_sales_income])
		lineageTag: b0f764c9-bd3f-4fdb-b740-11899d3b7a35

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	/// Bottler Gross Profit
	measure 'Bottler Gross Profit' =
			
			//[Dead Net Net Sales Income] - SUM('Customer Invoice Measures'[cogs])
			[Bottler Dead Net Net Sales Income with Proxy] - SUM('Customer Invoice Measures'[cogs])
		formatString: \$#,0;(\$#,0);\$#,0
		lineageTag: 453142e2-cac5-49cf-a95b-67fe67afc8eb

		annotation PBI_FormatHint = {"currencyCulture":"en-US"}

	/// Bottler Margin
	measure 'Bottler Margin' = DIVIDE([Bottler Gross Profit], [Bottler Revenue])
		formatString: #,0.00%;-#,0.00%;#,0.00%
		lineageTag: d5461d9d-d270-4598-a944-3e8a2f22781b

	measure 'SC SellingDays CY2' =
			
			Var CY = YEAR(TODAY())
			Var PY = CY-1
			var CYSellingDays = CALCULATE(DISTINCTCOUNT('Calendar'[dim_date_key]), 'Calendar'[selling_day_ind] = 1,'Calendar'[Fiscal Year] = CY)
			var PYSellingDays = CALCULATE(DISTINCTCOUNT('Calendar'[dim_date_key]), 'Calendar'[selling_day_ind] = 1,'Calendar'[Fiscal Year] = PY)
			
			
			VAR TimePeriod = SELECTEDVALUE('Time Calculation'[Time Calc Parent])
			RETURN
			    IF(
			        ISBLANK(TimePeriod),
			        CYSellingDays,  -- When no selection, return the CYSellingDays
			        CALCULATE(
			            [SellingDays],
			            'Time Calculation'[Time Calculation] = TimePeriod
			        )
			    )
		displayFolder: Temporary Measures - to be deleted
		lineageTag: 910593a6-1198-41cd-ad03-0a4665b1cc59

