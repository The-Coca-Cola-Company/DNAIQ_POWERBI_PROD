table 'Bottler Financials'
	lineageTag: 0ca21b68-81eb-492c-8298-33348c3a07d8

	/// Bottler Revenue (DNNSI)
	measure 'Dead Net Net Sales Income' = CALCULATE(SUM('Customer Invoice Measures'[bottler_dead_net_net_sales_income]))
		formatString: \$#,0.###############;(\$#,0.###############);\$#,0.###############
		lineageTag: a32242de-327b-40cb-8c54-fb3d871f00e6

		annotation PBI_FormatHint = {"currencyCulture":"en-US"}

	measure 'Dead Net Net Sales Income - Plan' = CALCULATE(SUM('Overall Performance Plan'[revenue_plan]))
		formatString: \$#,0;(\$#,0);\$#,0
		lineageTag: ce018cac-5860-4e81-a32e-5bbce05ea400

	measure 'Dead Net Net Sales Income - Forecast' = CALCULATE(SUM(Forecast[revenue_forecast]))
		formatString: \$#,0.###############;(\$#,0.###############);\$#,0.###############
		lineageTag: 99f1429a-7a59-4353-bedd-abe373c64b72

		annotation PBI_FormatHint = {"currencyCulture":"en-US"}

	measure 'NSR / UC' =
			
			DIVIDE(
			    [Dead Net Net Sales Income],
			    [Unit Cases])
		formatString: \$#,0;(\$#,0);\$#,0
		lineageTag: 796ff0f4-4a37-4976-9b0c-eff73630844d

	measure UCS/ADS = ```
			
			Var CY = YEAR(TODAY())
			Var PY = CY-1
			VAR ytd_week = 
			    CALCULATE(
			        max('Calendar'[Week Number]),
			        'Calendar'[Fiscal Year] = CY)
			
			var CYSellingDays = [SC SellingDays CY2]
			
			var PYSellingDays = [SC SellingDays PY2]
			
			var AdjRatio = divide(CYSellingDays,PYSellingDays)
			
			var CYVolume = [SC Unit Cases CY]
			var PYVolume = [SC Unit Cases PY]
			
			var ADS = DIVIDE(([SC Unit Cases CY] - [SC Unit Cases PY]*[SC Adj Ratio]),[SC Unit Cases PY]*[SC Adj Ratio])
			
			RETURN 
			ADS
			```
		formatString: 0.0%;-0.0%;0.0%
		lineageTag: 8d0118cb-4fbb-4744-8277-ee9ec654a3c1

	measure 'Bottler Gross Profit / EQ Case' = divide([Bottler Gross Profit],[Unit Cases])
		lineageTag: c63e3ed8-6bfd-40eb-aa5f-ea8d0b8dbd4d

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Bottler Gross Profit / EQ Case - Plan' = divide([Bottler Gross Profit],[Unit Cases - Plan])
